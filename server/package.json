    "express": "~4.16.1",
    "express-session": "^1.17.1",
    "helmet": "^4.2.0",
    "http-errors": "~1.6.3",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.21.12",
    "morgan": "~1.9.1",
    "passport": "^0.4.1",
    "passport-auth0": "^1.4.0",
    "pg": "^8.5.1",
